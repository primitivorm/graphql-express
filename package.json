{
  "name": "graphql-express",
  "version": "1.0.0",
  "description": "Demo for GraphQL and Express",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/ava tests/**/*.js --verbose",
    "test:watch": "node_modules/.bin/ava --verbose --watch",
    "start": "nodemon server/index.js --watch server --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primitivorm/graphql-express.git"
  },
  "author": "Primitivo RM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/primitivorm/graphql-express/issues"
  },
  "homepage": "https://github.com/primitivorm/graphql-express#readme",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  },
  "dependencies": {
    "ava": "^1.2.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "backpack-core": "^0.8.3",
    "body-parser": "^1.18.3",
    "casual": "^1.6.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "graphql-server-express": "^1.4.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "http": "0.0.0",
    "lodash": "^4.17.11",
    "sequelize": "^4.42.0",
    "sqlite3": "^4.0.6",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "nodemon": "^1.18.9"
  }
}
